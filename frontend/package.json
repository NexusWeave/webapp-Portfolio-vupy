    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.35.0",
    "nuxt": "^4.1.2",
    "tinacms": "^2.8.3",
    "typescript": "^5.9.2",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@pinia/nuxt": "^0.5.5",
    "@tinacms/cli": "^1.10.3",
    "@types/node": "^24.10.0",
